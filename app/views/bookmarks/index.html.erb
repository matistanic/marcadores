<p id="notice"><%= notice %></p>

<h1>Bookmarks</h1>

<div class="m-4 bordered-1">
  <%= render 'form' , bookmark: @bookmark%>
</div>

<table class="table table-bordered" id="table_bookmarks">
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th>Type</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @bookmarks %>
  </tbody>
</table>

<br>

<%= link_to 'New Bookmark', new_bookmark_path %>